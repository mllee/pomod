{
	"manifest_version": 2,
	"name": "Pomo Timer",
	"description": "Set a timer",
	"version": "0.1",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": ["background"],
    "background": {
    	"js": ["jquery.min.js"],
    	"scripts": ["background.js"]
    	//"persistent": true
    },
    "web_accessible_resources": ["script.js"]
}