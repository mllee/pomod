{
	"manifest_version": 2,
	"name": "Pomo Timer",
	"description": "Set a timer",
	"version": "0.1",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": ["background", "storage", "declarativeContent", "tabs"],
    "background": {
    	//"page": "background.html",
    	"js": "jquery.min.js",
    	"scripts": ["background.js"]
    },
    "web_accessible_resources": ["script.js"]
}
