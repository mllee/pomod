{
	"manifest_version": 2,
	"name": "Pomo Timer",
	"description": "Set a timer",
	"version": "0.1",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [{
        "js": ["jquery.min.js", "contentscript.js"],
        "matches": ["<all_urls>"]
    }],
    "background": {
    	"scripts": ["eventPage.js"],
    	"persistent": false
    },
    "web_accessible_resources": ["script.js"]
}